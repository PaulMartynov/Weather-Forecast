{
  "name": "otus-homework-5",
  "version": "1.0.0",
  "description": "Weather web-app",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "prettier --check . && eslint . --ext .js,.ts && tsc --noEmit",
    "lint:fix": "prettier --write . && eslint . --fix --ext .js,.ts",
    "make-badges": "istanbul-badges-readme",
    "prepare": "husky install",
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulMartynov/otus-homework-5.git"
  },
  "author": "Pavel Martynov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulMartynov/otus-homework-5/issues"
  },
  "homepage": "https://github.com/PaulMartynov/otus-homework-5#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.14.8",
    "@types/jest": "^26.0.23",
    "css-loader": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.27.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "istanbul-badges-readme": "^1.4.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "2.3.2",
    "style-loader": "^3.2.1",
    "typescript": "^4.3.5",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix --ext .js,.ts",
    "*.{js,css,md,ts,json}": "prettier --write"
  }
}
