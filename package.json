{
  "name": "otus-homework-5",
  "version": "1.0.0",
  "description": "Weather web-app",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "prettier --check . && eslint .",
    "lint:fix": "prettier --write . && eslint . --fix",
    "prepare": "husky install",
    "build": "webpack --mode=production",
    "start": "webpack serve --mode=development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PaulMartynov/otus-homework-5.git"
  },
  "author": "Pavel Martynov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PaulMartynov/otus-homework-5/issues"
  },
  "homepage": "https://github.com/PaulMartynov/otus-homework-5#readme",
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@types/jest": "^26.0.23",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.0.1",
    "prettier": "2.3.2",
    "webpack": "^5.47.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{css,js,md,yml,html}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
